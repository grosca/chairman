PREFIX mse:<http://yarta.gforge.inria.fr/ontologies/mse.rdf#> PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>CONSTRUCT { %subject% %predicate% %object% } where { ?req rdf:type mse:requestor . ?req mse:performs mse:read . FILTER(REGEX(STR(%predicate%), "^http://yarta.gforge.inria.fr/ontologies/chairman.rdf#")) %subject% %predicate% %object% . }
PREFIX mse:<http://yarta.gforge.inria.fr/ontologies/mse.rdf#> PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>CONSTRUCT { %subject% %predicate% %object% } where { ?req rdf:type mse:requestor . ?req mse:performs mse:add . FILTER(REGEX(STR(%predicate%), "^http://yarta.gforge.inria.fr/ontologies/chairman.rdf#"))}
PREFIX mse:<http://yarta.gforge.inria.fr/ontologies/mse.rdf#> PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>CONSTRUCT { %subject% %predicate% %object% } where { ?req rdf:type mse:requestor . ?req mse:performs mse:remove . FILTER(REGEX(STR(%predicate%), "^http://yarta.gforge.inria.fr/ontologies/chairman.rdf#")) %subject% %predicate% %object% . }